{
  "name": "omekas-to-wikidata",
  "version": "1.0.0",
  "description": "A tool for mapping Omeka S data to Wikidata",
  "main": "src/js/app.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": ["js", "json"],
    "moduleNameMapper": {
      "^/js/(.*)$": "<rootDir>/src/js/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ]
  },
  "babel": {
    "presets": [
      ["@babel/preset-env", { "targets": { "node": "current" } }]
    ]
  },
  "author": "",
  "license": "MIT"
}